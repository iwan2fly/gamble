<script>
    let mainVue;
    document.addEventListener("DOMContentLoaded", function () {
        mainVue = new Vue({
            el: '#el-main',
            created() {
                log('created main vue');
            },
            methods: {
                async callTest() {
                    const response = await Promise.all([
                        getOnlyAdmin({
                            isAdmin: true,
                        }),
                        getOnlyManager(),
                        getOnlyUser(),
                    ]);
                    log('# onlyAdmin() response: ', response[0].data.data.who);
                    log('# onlyManager() response: ', response[1].data.data.who);
                    log('# onlyUser() response: ', response[2].data.data.who);
                }
            }
        });
    });
</script>

